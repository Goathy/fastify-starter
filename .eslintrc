{
    "extends": "standard-with-typescript",
    "parserOptions": {
        "project": "./tsconfig.eslint.json"
    },
    "rules": {
        "import/order": [
            "error",
            {
                "newlines-between": "always",
                "alphabetize": {
                    "order": "asc"
                }
            }
        ]
    }
}